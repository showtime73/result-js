{
	"name": "result-js",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "packages/result-js/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "@nrwl/esbuild:esbuild",
			"outputs": ["{options.outputPath}"],
			"options": {
				"outputPath": "dist/packages/result-js",
				"main": "packages/result-js/src/index.ts",
				"tsConfig": "packages/result-js/tsconfig.lib.json",
				"assets": ["packages/result-js/*.md"],
				"minify": true,
				"format": ["esm", "cjs"]
			}
		},
		"publish": {
			"executor": "nx:run-commands",
			"options": {
				"command": "node tools/scripts/publish.mjs result-js {args.ver} {args.tag}"
			},
			"dependsOn": ["build"]
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "packages/result-js/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"typedoc": {
			"executor": "@enio.ai/typedoc:typedoc",
			"options": {
				"options": "typedoc.json"
			},
			"configurations": {}
		}
	},
	"tags": []
}
