{
	"name": "@tongtwist/result-js",
	"version": "0.1.0",
	"author": {
		"name": "Tongtwist",
		"url": "https://github.com/tongtwist"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tongtwist/result-js.git"
	},
	"type": "commonjs",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"directories": {
		"src": "./src",
		"lib": "./lib"
	},
	"files": [
		"./lib/**/*",
		"./docs/**/*"
	],
	"keywords": [
		"typescript",
		"result",
		"success",
		"failure",
		"ok",
		"error"
	],
	"scripts": {
		"build:tsc": "tsc",
		"build:dev": "run-s clean build:tsc test",
		"build": "npm run build:dev",
		"bundle": "node ./bundle.js",
		"clean:docs": "rimraf docs",
		"clean:lib": "rimraf lib",
		"clean:spec-js": "rimraf lib/**/*.spec.js && rimraf lib/**/*.js.map",
		"clean": "run-p clean:docs clean:lib",
		"preversion": "run-s bundle",
		"version": "git add -A src lib docs README.md && git commit -m \"Bump version to v$npm_package_version\"",
		"postversion": "git push && git push --tags",
		"publish:patch": "npm version patch && npm publish",
		"publish:minor": "npm version minor && npm publish",
		"publish:major": "npm version major && npm publish",
		"test:publish": "jest --no-cache --passWithNoTests --coverage --json > coverage.json",
		"test": "jest --no-cache --passWithNoTests",
		"moveCoverage": "mv coverage docs/",
		"watch:src": "tsc --watch --noEmit",
		"watch": "run-p watch:*"
	},
	"engines": {
		"node": ">= 16.0.0"
	},
	"devDependencies": {
		"@jest/globals": "^29.3.1",
		"@theloop/sheldon-node-module": "^0.2.0",
		"@types/jest": "^29.2.4",
		"@types/node": "^18.11.18",
		"jest": "^29.3.1",
		"npm-run-all": "^4.1.5",
		"rimraf": "^3.0.2",
		"ts-jest": "^29.0.3",
		"ts-node": "^10.9.1",
		"typescript": "~5.0.2"
	},
	"dependencies": {
		"zod": "~3.21.4"
	}
}
